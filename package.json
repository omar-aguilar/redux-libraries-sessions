{
  "name": "redux-libraries",
  "version": "1.0.0",
  "description": "Sample code for different redux libraries",
  "main": "index.js",
  "scripts": {
    "exec": "babel-node -x .ts,.js",
    "build": "npm run build:dev",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "lint": "eslint --color --ext .js,.ts,.tsx src *.js .eslintrc.js",
    "start": "webpack-dev-server --mode development",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "ts:check-types": "tsc",
    "verify": "npm run lint && npm run ts:check-types && npm run stylelint"
  },
  "keywords": [
    "redux",
    "libraries"
  ],
  "author": "Omar Aguilar",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.7",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.6.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-observable": "^2.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "rxjs": "^7.5.5"
  }
}
